{
    "name": "Coursera Advisor",
    "description": "Get accurate ratings in Coursera",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":
    [
        "storage",
        "scripting"
    ],
    "host_permissions":
    [
        "https://www.coursera.org/"
    ],
    "action":
    {
        "default_popup": "popup.html",
        "default_icon":
        {
            "16": "/images/logo_16.png",
            "32": "/images/logo_32.png",
            "48": "/images/logo_48.png",
            "128": "/images/logo_128.png"
        }
    },
    "icons":
    {
        "16": "/images/logo_16.png",
        "32": "/images/logo_32.png",
        "48": "/images/logo_48.png",
        "128": "/images/logo_128.png"
    },
    "default_locale": "en"
}