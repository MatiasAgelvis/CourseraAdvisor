{
	"name": "Stellaid",
	"version": "0.0.0",
	"description": "Verify Star Ratings anywhere!",
	"homepage_url": "https://github.com/refined-github/refined-github",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "awesome-extension@notlmn.github.io",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"https://github.com",
		"https://www.coursera.org",
		"storage"
	],
	"content_scripts": [ 
		{
			"matches": [ "https://www.coursera.org/*" ],
			"js": [ "coursera.js" ],
			"run_at": "document_idle"
		},
		{
			"matches": [ "https://github.com/*" ],
			"js": [ "content.js" ],
			"css": [ "content.css" ],
			"run_at": "document_end"
		}
 	],
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	}
}
